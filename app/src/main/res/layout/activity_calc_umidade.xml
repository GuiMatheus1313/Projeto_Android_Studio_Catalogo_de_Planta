<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_CalcUmidade">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:background="@color/Fundo_app"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <TextView
            android:id="@+id/textView_Umidade1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="50sp"
            android:fontFamily="@font/galada"
            android:text="@string/Vazio"
            android:textColor="@color/black"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/textView_Umidade2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="50sp"
            android:fontFamily="@font/galada"
            android:text="@string/Vazio"
            android:textColor="@color/black"
            android:textSize="10sp"
            tools:fontFamily="@font/galada" />

        <TextView
            android:id="@+id/textView_Umidade3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="50sp"
            android:fontFamily="@font/galada"
            android:text="@string/Vazio"
            android:textColor="@color/black"
            android:textSize="10sp" />

        <Button
            android:id="@+id/btnCalcUmi_Voltar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="380sp"
            android:text="@string/RetornodoCadastro" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>